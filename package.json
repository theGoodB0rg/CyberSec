{
  "name": "cybersecurity-web-app",
  "version": "1.0.0",
  "description": "Comprehensive Cybersecurity Web App with Interactive Terminal and SQLMap Integration",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build && npm run server:build",
    "start": "node server/index.js",
    "start:prod": "node server/dist/index.js",
    "server:dev": "nodemon server/index.js",
    "server:build": "node -p \"require('fs').mkdirSync('server/dist', {recursive: true})\" && node -p \"require('fs').copyFileSync('server/index.js', 'server/dist/index.js')\"",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:lint": "npm --prefix client run lint",
    "lint:all": "npm run lint && npm run client:lint",
    "client:install": "cd client && npm install",
    "server:install": "npm install",
    "install-all": "npm run server:install && npm run client:install",
    "test": "jest",
    "lint": "eslint server/**/*.js",
    "format": "prettier --write server/**/*.js"
  },
  "keywords": [
    "cybersecurity",
    "sqlmap",
    "penetration-testing",
    "web-terminal",
    "security-audit"
  ],
  "author": "Professional Developer",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "glob": "^11.0.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "markdown-pdf": "^10.0.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^24.11.1",
    "sanitize-html": "^2.11.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
  "validator": "^13.11.0",
  "winston": "^3.11.0",
  "axios": "^1.7.2",
  "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
